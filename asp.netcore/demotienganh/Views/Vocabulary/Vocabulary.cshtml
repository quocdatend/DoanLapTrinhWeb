@{
    ViewData["Title"] = "Vocabulary Page";
    Layout = "~/Views/Shared/_LayoutMain.cshtml";
}

@model List<Vocabulary>

<h2>Vocabulary</h2>
<table style="border: solid red; margin: 80px auto">
    <tr>
        <th style="border: solid red">
            Tên tiếng anh
        </th>
        <th style="border: solid red">
            Tên tiếng việt
        </th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <th style="border: solid red">
                @item.NameEn
            </th>
            <th style="border: solid red">
                @item.NameVn
            </th>
        </tr>
    }

</table>
